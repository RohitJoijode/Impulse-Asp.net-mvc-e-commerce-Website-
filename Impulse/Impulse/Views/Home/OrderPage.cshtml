    <section class="padding-y bg-light">
        <div class="container">

            <div class="row">
                @if (Model.UserId != null)
                {
                <aside class="col-lg-3 col-xl-3">
                    <!--  COMPONENT MENU LIST  -->
                     <nav class="nav flex-lg-column nav-pills mb-4">
                        <a class="nav-link active" href="#">Account main</a>
                        <a class="nav-link" onclick="OrdersDetailsView(2);">New Orders</a>
                        <a class="nav-link" onclick="OrdersDetailsView(1);">Pending Orders</a>
                        <a class="nav-link" onclick="OrdersDetailsView(3);">Orders history</a>
                        <a class="nav-link" href="/LogIn/LogOut">Log out</a>
                    </nav>
                    <!--   COMPONENT MENU LIST END .//   -->
                </aside>
                }
                @if (Model.UserId != null)
                {
                    <main class="col-lg-9  col-xl-9">
                        <article class="card">
                            <div class="content-body">

                                <figure class="itemside align-items-center">
                                    @*<div class="aside">
                                            <span class="bg-gray icon-md rounded-circle">
                                                <img src="images/avatars/avatar.webp" class="icon-md rounded-circle">
                                            </span>
                                        </div>*@
                                    <figcaption class="info">
                                        <h6 class="title">@Model.UserFullName</h6>
                                        <p>
                                            Email: @Model.Email, Phone: +@Model.UserMobileNumber
                                            <a href="/Home/EditRegisterProfile" class="px-2"><i class="fa fa-pen"></i></a>
                                        </p>
                                    </figcaption>
                                </figure>
                                <hr>
                                <div class="row g-2 mb-3">
                                    <div class="col-md-6">
                                        <a class="btn btn-light">@*<i class="me-2 fa fa-plus"></i>*@Your Delivery Address</a>
                                        <br>
                                        <br>
                                        
                                        <article class="box bg-light">
                                            <b class="mx-2 text-muted"><i class="fa fa-map-marker-alt"></i></b>
                                            @Model.UserAddress
                                        </article>

                                    </div> <!-- col.// -->
                                    <!-- col.// -->
                                </div> <!-- row.// -->
                                @*<a href="#" class="btn btn-light"> <i class="me-2 fa fa-plus"></i> Add new address </a>*@
                                <hr class="my-4">
                                <div id="OrdersPartialViewsId">
                                   
                                </div>
                            </div>
                            <!-- card-body .// -->
                        </article> <!-- card .// -->
                    </main>
                }
                else
                {
                    <main class="col-lg-9  col-xl-9">
                        <article class="card">
                            <table class="content-body">
                                <tr>
                                    <td>
                                        <h6>Please Sign In For showing Order</h6>
                                    </td>
                                    <td>
                                        <a href="/LogIn/LogIn" class="btn btn-primary">Sign In</a>
