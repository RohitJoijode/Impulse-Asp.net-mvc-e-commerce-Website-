        if (Data.UserType == undefined) {
            ShowModalMessage("Please Select User");
            return false;
        }
        //alert(Data.UserType);

        Data.Address = Address;
        if (Data.Address == "") {
            ShowModalMessage("Please Insert Address Field");
            return false;
        }

        Data.UserCoutryId = Country;
        if (Data.UserCoutryId == "" || Data.UserCoutryId == 0) {
            ShowModalMessage("Please Select Country");
            return false;
        }

        Data.UserStateId = State;
        if (Data.UserStateId == "" || Data.UserStateId == 0) {
            ShowModalMessage("Please Select State");
            return false;
        }

        Data.UserCityId = City;
        if (Data.UserCityId == "" || Data.UserCityId == 0) {
            ShowModalMessage("Please Select City");
            return false;
        }

        Data.PinCode = PinCode;
        if (Data.PinCode == "") {
            ShowModalMessage("Please Insert PinCode");
            return false;
        }

        Data.UserType = UserType;
        if (Data.UserType == "") {
            ShowModalMessage("Please Select User Type");
            return false;
        }
        Data.IsKeepMeUpToDateOnNews = KeepMeUpToDateOnNews;

        Data.IsRegisterPageEdit = true;

        $.ajax({
            type: "POST",
            url: "/LogIn/SubmitOTPForRegisterPage",
            data: JSON.stringify(Data),
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (Response) {
                if (Response.IsSuccess == true) {
                    window.location.href = "/LogIn/LogIn";
                } else if (Response.IsSuccess == false) {
                    ShowModalMessage("Something went wrong...")
                }
            }
        });
    }
    </script>
