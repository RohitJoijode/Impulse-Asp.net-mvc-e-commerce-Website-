    <script>

    var UserId = @Html.Raw(Json.Encode(Model.UserId));
    var WistListCount = @Html.Raw(Json.Encode(Model.WistListCount));
    var AddToCartCount = @Html.Raw(Json.Encode(Model.AddToCartCount));
    var OrderListCount = @Html.Raw(Json.Encode(Model.OrderListCount));

    if (AddToCartCount == null) {
        document.getElementById('MyCartButtonCountId').innerHTML = "";
        document.getElementById('MyCartButtonCountId').innerHTML = '0';
    } else if (AddToCartCount != null && AddToCartCount != undefined) {
        document.getElementById('MyCartButtonCountId').innerHTML = "";
        document.getElementById('MyCartButtonCountId').innerHTML = AddToCartCount;
    }

    if (WistListCount != null && WistListCount != undefined) {
        document.getElementById('WishlistButtonCountId').innerHTML = "";
        document.getElementById('WishlistButtonCountId').innerHTML = WistListCount;
    } else if (WistListCount == null) {
        document.getElementById('WishlistButtonCountId').innerHTML = "";
        document.getElementById('WishlistButtonCountId').innerHTML = '0';
    }

    if (OrderListCount != null && OrderListCount != undefined) {
        document.getElementById('OrderPageCountId').innerHTML = "";
        document.getElementById('OrderPageCountId').innerHTML = OrderListCount;
    }
    else if (OrderListCount == null)
    {
        document.getElementById('OrderPageCountId').innerHTML = "";
        document.getElementById('OrderPageCountId').innerHTML = '0';
    }

    var RegisterGeneratedOTP = "";

    function CancelRegisterDetails() {
        debugger;
        $('#FirstNameId').val('');
        $('#LastNameId').val('');
        $('#PhoneId').val('');
        $('#PasswordId').val('');
        $('#ConfirmPasswordId').val('');
        $('#IsKeepMeUpToDateOnNewsId').prop('checked', false);
        $('#AddressId').val('');
        $('#CountryId').get(0).selectedIndex = 0;
        $('#StateId').get(0).selectedIndex = 0;
        $('#CityId').get(0).selectedIndex = 0;
        $('#PinCodeId').val('');
    }

        var UserCoutryId =  @Html.Raw(Json.Encode(Model.UserCoutryId));
        var UserStateId =  @Html.Raw(Json.Encode(Model.UserStateId));
        var UserCityId =  @Html.Raw(Json.Encode(Model.UserCityId));


        GetStateListByCountryId(UserCoutryId);
        GetCityListByCountryIdAndStateId(UserCoutryId,UserStateId);

    function GetStateListByCountryId(CountryId) {
        debugger;
        Data = {};
        if (CountryId != null && CountryId != undefined) {
            Data.CountryId = CountryId;
        } else {
            Data.CountryId = $('#CountryId :selected').val();;
        }
        $.ajax({
            type: "POST",
            url: "/Register/GetStateListByCountryId",
            data: JSON.stringify(Data),
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (Response) {
                $("#StateId").empty();
                $("#StateId").append($("<option></option>").val(0).html('-- Select State --'));
                $("#CityId").empty();
                $("#CityId").append($("<option></option>").val(0).html('-- Select City --'));
                if (Response != null) {
                    $.each(Response, function (data, value) {
                        $("#StateId").append($("<option></option>").val(value.Id).html(value.StateName));
                    })
                    if (UserStateId != null) {
                        $("#StateId").val(UserStateId);
                    }
                }
            }
        });
    }


    function GetCityListByCountryIdAndStateId(CountryId,StateId) {
        debugger;
        alert('CountryId');
        alert(CountryId);

        alert('StateId');
        alert(StateId);

        Data = {};
        if (CountryId != null && CountryId != undefined) {
            Data.CountryId = CountryId;
        } else {
            Data.CountryId = $('#CountryId :selected').val();;
        }

        if (StateId != null && StateId != undefined) {
            Data.StateId = StateId;
        } else {
            Data.StateId = $('#StateId :selected').val();
        }

        $.ajax({
            type: "POST",
            url: "/Register/GetCityListByCountryIdAndStateId",
            data: JSON.stringify(Data),
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (Response) {
                if (Response != null) {
                    $("#CityId").empty();
                    $("#CityId").append($("<option></option>").val(0).html('-- Select City --'));
                    $.each(Response, function (data, value) {
                        $("#CityId").append($("<option></option>").val(value.Id).html(value.CityName));
                    })

                    if (UserCityId != null) {
                        $("#CityId").val(UserCityId);
                    }

                }
            }
        });
    }

    function UpdateRegistrationForm() {
        debugger;
        /*document.getElementById("ConfirmPasswordErrorId").innerHTML = "";*/
        RegisterGeneratedOTP = "";
        var Data = {};
        var Firstname = $('#FirstNameId').val();
        var LastName = $('#LastNameId').val();
        var Phone = $('#PhoneId').val();
        var KeepMeUpToDateOnNews = false;
        if ($('#IsKeepMeUpToDateOnNewsId').is(":checked")) {
            KeepMeUpToDateOnNews = true;
        } else {
            KeepMeUpToDateOnNews = false;
        }
        var UserType = $('input[name="dostavka"]:checked').val();
        var Email = $('#EmailId').val();
        var Address = $('#AddressId').val();
        var Country = $('#CountryId :selected').val();
        var State = $('#StateId :selected').val();
        var City = $('#CityId :selected').val();
        var PinCode = $('#PinCodeId').val();
        var password = $('#PasswordId').val();
        var confirmPassword = $('#ConfirmPasswordId').val();

        Data.Firstname = Firstname;
        if (Data.Firstname == "") {
            ShowModalMessage("Please Insert First Name");
            return false;
        }

        Data.LastName = LastName;
        if (Data.LastName == "") {
            ShowModalMessage("Please Insert Last Name Field");
            return false;
        }

        Data.Phone = Phone;
        alert(Data.Phone);
        if (Data.Phone == "") {
            ShowModalMessage("Please Insert Phone Number Field");
            return false;
        }

        Data.Email = Email;
        if (Data.Email == "") {
            ShowModalMessage("Please Insert Email Field");
            return false;
        }

        Data.password = password;
        alert(Data.password);
        if (Data.password == "") {
            ShowModalMessage("Please Insert Password Field");
            return false;
        }

        Data.confirmPassword = confirmPassword;
        alert(Data.confirmPassword);
        if (Data.confirmPassword == "") {
            ShowModalMessage("Please Insert Confirm Password Field");
            return false;
        }

        if (password != confirmPassword) {
            ShowModalMessage("Passwords do not match.");
            return false;
        }

        Data.UserType = UserType;
        if (Data.UserType == undefined) {
            ShowModalMessage("Please Select User");
            return false;
        }
        //alert(Data.UserType);

        Data.Address = Address;
        if (Data.Address == "") {
            ShowModalMessage("Please Insert Address Field");
            return false;
        }

        Data.UserCoutryId = Country;
        if (Data.UserCoutryId == "" || Data.UserCoutryId == 0) {
            ShowModalMessage("Please Select Country");
            return false;
        }

        Data.UserStateId = State;
        if (Data.UserStateId == "" || Data.UserStateId == 0) {
            ShowModalMessage("Please Select State");
            return false;
        }

        Data.UserCityId = City;
        if (Data.UserCityId == "" || Data.UserCityId == 0) {
            ShowModalMessage("Please Select City");
            return false;
        }

        Data.PinCode = PinCode;
        if (Data.PinCode == "") {
            ShowModalMessage("Please Insert PinCode");
            return false;
        }

        Data.UserType = UserType;
        if (Data.UserType == "") {
            ShowModalMessage("Please Select User Type");
            return false;
        }

        Data.IsKeepMeUpToDateOnNews = KeepMeUpToDateOnNews;

        $.ajax({
            type: "POST",
            url: "/LogIn/UpdateRegisterPage",
            data: JSON.stringify(Data),
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (Response) {
                if (Response.IsSuccess == true && Response.ResponseMessage == "Send Mail Successfully...") {
                    $('#myModal').modal({
                        backdrop: 'static',
                        keyboard: true,
                        show: true
                    });
                    $('#myModal').modal('show');
                    RegisterGeneratedOTP = Response.RegisterOTP;
                } else if (Response.IsSuccess == false) {
                    ShowModalMessage("Something went wrong...")
                }
            }
        });
    }

