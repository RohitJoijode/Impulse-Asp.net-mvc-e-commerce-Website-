 function OnClickAddToCart(SubCategoryId,CategoryId) {
     debugger;
     var Data = {};

     if (UserId == null) {
         ShowModalForLogIn();
         return false;
     }

    Data.SubCategoryId = SubCategoryId,
    Data.CategoryId = CategoryId,
     $.ajax({
         type: 'POST',
         dataType: 'JSON',
         data: JSON.stringify(Data),
         contentType: "application/json;charset=utf-8",
         url: '/Category/SaveToAddToCart',
         success:
             function (response) {
                 if (response.IsSuccess == true) {
                     document.getElementById('MyCartButtonCountId').innerHTML = "";
                     document.getElementById('MyCartButtonCountId').innerHTML = response.AddToCartCount;
                     document.getElementById('WishlistButtonCountId').innerHTML = "";
                     document.getElementById('WishlistButtonCountId').innerHTML = response.WishListCount;
                     swal({
                         title: "Add To Cart SuccessFul",
                         //text: "You clicked the button!",
                         icon: "success",
                         button: "Ok",
                     });
                 }
             },
     });
    }

