<script>
    $(document).ready(function () {
        $.ajax({
            type: 'POST',
            dataType: 'JSON',
            url: '/Category/GetTopDealsSubCategoryListOnTopDealPage',
            success:
                function (response) {
                    if (response.IsSuccess == true) {
                        $('#TopDealsGridId').empty();
                        $('#TopDealsGridId').html(response.StringReponse);
                    }
                },
        });
    });

    function SearchFilterOnTopDealsPage() {
        debugger;
        var min = $('#minId').val();
        var max = $('#maxId').val();
        var stars = $('input[name="FilterStars"]').val();
        var Data = {};
        //$.cookie("SomeID", 1);
        //$.cookie("example", "foo", { expires: 7});
        Data.Min = min,
        Data.Max = max,
        Data.Stars = stars,

        $.ajax({
            type: 'POST',
            dataType: 'JSON',
            data: JSON.stringify(Data),
            contentType: "application/json;charset=utf-8",
            url: '/Category/SearchFilterOnTopDealsPage',
            success:
                function (response) {
                    //var SomeID = getCookie("SomeID");
                    //alert(SomeID);
                    //console.log(SomeID);
                    ////$.cookie("cart");
                    //$.removeCookie("SomeID");
                    if (response.IsSuccess == true) {
                        $('#TopDealsGridId').empty();
                        $('#TopDealsGridId').html(response.StringReponse);
                    }
                },
        });

    }

</script>

