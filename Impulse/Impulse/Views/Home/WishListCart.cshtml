    function OnClickAddToCart(SC,WistListUniqueId,Sub_Category_Price,Sub_Category_ColorId,Sub_Category_SizeId) {
        debugger;
        try {
            var Data = {};
            if (UserId == null) {
                ShowModalForLogIn();
                return false;
            }
            Data.Id = WistListUniqueId;
            Data.Sub_CategoryId = SC;
            var quantityId =  $('#QuantityId' + WistListUniqueId + ' option:selected').val();
            Data.Quantity = parseInt(quantityId);
            Data.Sub_Category_Price = Sub_Category_Price;
            Data.Sub_Category_ColorId = Sub_Category_ColorId;
            Data.Sub_Category_SizeId = Sub_Category_SizeId;
            Data.IsWishList = true;
            $.ajax({
                url: '/Category/SaveToAddToCartOnWistListPage',
                type: 'POST',
                dataType: 'JSON',
                data: JSON.stringify(Data),
                contentType: "application/json;charset=utf-8",
                success:
                    function (response) {
                        if (response.IsSuccess == true) {
                            GetWistListDataOnWistListPage1();
                        }
                    },
            });
        } catch (error) {
            alert(error);
        }
    }

