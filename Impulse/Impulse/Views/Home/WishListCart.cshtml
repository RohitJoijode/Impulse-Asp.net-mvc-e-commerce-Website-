    <!-- ==============Begin Showing Modal For Log In// ============== -->
    @using (Html.BeginForm("Submit", "LogIn", FormMethod.Post))
    {
        <div class="container">
            <div class="modal fade" id="myModal" role="dialog" data-backdrop="static" data-keyboard="false">
                <div class="modal-dialog">
                    <div class="modal-header">
                    </div>
                    <!-- Modal content-->
                    <div class="modal-content">
                        <div class="modal-body">
                            <div class="card mb-3">
                                <div class="card-body">
                                    <div class="pt-4 pb-2">
                                        <h5 class="card-title text-center pb-0 fs-4">Login</h5>
                                    </div>
                                    <table class="col-12">
                                        <tr>
                                            <td>
                                                <label for="yourUsername">Username</label>
                                                @Html.TextBoxFor(m => m.UserName, new { @name = "username", @type = "text", @id = "UserName", @class = "form-control", @Placeholder = "example@gmail.com" })
                                                @Html.ValidationMessageFor(m => m.UserName)
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label for="yourPassword">Password</label>
                                                @Html.TextBoxFor(m => m.UserPassword, new { @name = "password", @type = "password", @id = "UserPassword", @class = "form-control", @Placeholder = "Password" })
                                                @Html.ValidationMessageFor(m => m.UserPassword)
                                            </td>
                                        </tr>
                                    </table>
                                    <hr />
                                    <div class="col-12">
                                        <button class="btn btn-primary w-100" type="submit">LogIn</button>
                                    </div>
                                    <div class="col-12" style="padding-top:3px;">
                                        <button class="btn btn-danger w-100" type="button" onclick="closeModalForLogIn();">Cancel</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    }

